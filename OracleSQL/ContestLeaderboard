Score:30|Diff:Med

SELECT HID,NAME,SUM(SCORE) FROM
(SELECT DISTINCT HACKER_ID HID,(SELECT NAME FROM HACKERS WHERE HACKER_ID=S.HACKER_ID) NAME,CHALLENGE_ID,MAX(SCORE) OVER(PARTITION BY HACKER_ID,CHALLENGE_ID) SCORE
FROM SUBMISSIONS S)
GROUP BY HID,NAME
HAVING SUM(SCORE)!=0
ORDER BY SUM(SCORE) DESC,HID;
